	<queryString language="SQL">
		<![CDATA[SELECT
     expedientemedico.`id` AS expedientemedico_id,
     expedientemedico.`idPaciente` AS expedientemedico_idPaciente,
     expedientemedico.`idAntecedentesPersonales` AS expedientemedico_idAntecedentesPersonales,
     expedientemedico.`idAntecedentesFamiliares` AS expedientemedico_idAntecedentesFamiliares,
     paciente.`id` AS paciente_id,
     paciente.`nombrePaciente` AS paciente_nombrePaciente,
     paciente.`apellido1Paciente` AS paciente_apellido1Paciente,
     paciente.`apellido2Paciente` AS paciente_apellido2Paciente,
     paciente.`sexo` AS paciente_sexo,
     paciente.`fechaNacimientoPaciente` AS paciente_fechaNacimientoPaciente,
     paciente.`nacionalidadPaciente` AS paciente_nacionalidadPaciente,
     paciente.`cedulaPaciente` AS paciente_cedulaPaciente,
     paciente.`idtelefono` AS paciente_idtelefono,
     paciente.`direccion1` AS paciente_direccion1,
     paciente.`direccion2` AS paciente_direccion2,
     paciente.`email` AS paciente_email,
     paciente.`tipo` AS paciente_tipo,
     antecedentespersonales.`id` AS antecedentespersonales_id,
     antecedentespersonales.`idAlergias` AS antecedentespersonales_idAlergias,
     antecedentespersonales.`tratamiento` AS antecedentespersonales_tratamiento,
     antecedentespersonales.`idVacunas` AS antecedentespersonales_idVacunas,
     antecedentespersonales.`medicamento` AS antecedentespersonales_medicamento,
     antecedentesfamiliares.`id` AS antecedentesfamiliares_id,
     antecedentesfamiliares.`parentesco` AS antecedentesfamiliares_parentesco,
     antecedentesfamiliares.`descripcion` AS antecedentesfamiliares_descripcion
FROM
     `paciente` paciente INNER JOIN `expedientemedico` expedientemedico ON paciente.`id` = expedientemedico.`idPaciente`
     INNER JOIN `antecedentespersonales` antecedentespersonales ON expedientemedico.`idAntecedentesPersonales` = antecedentespersonales.`id`
     INNER JOIN `antecedentesfamiliares` antecedentesfamiliares ON expedientemedico.`idAntecedentesFamiliares` = antecedentesfamiliares.`id`]]>
	</queryString>
